{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "An easy start to build-up the react components, a prototype wrap project for building react apps.",
  "main": "./src/app.js",
  "scripts": {
    "start-dev": "NODE_PATH=\"./src\" NODE_ENV=\"development\" node ./babel.server",
    "watch-client": "UV_THREADPOOL_SIZE=100 NODE_ENV=development node webpack-dev-server.js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonli/react-starter.git"
  },
  "keywords": [
    "react",
    "starter"
  ],
  "author": "Leon Li <lilei.leon@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonli/react-starter/issues"
  },
  "homepage": "https://github.com/leonli/react-starter#readme",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-plugin-typecheck": "0.0.3",
    "body-parser": "^1.12.0",
    "compression": "^1.3.0",
    "express": "^4.13.0",
    "react": "^0.13.3",
    "react-router": "v1.0.0-beta2",
    "url-loader": "^0.5.6",
    "file-loader": "^0.8.4"
  },
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
